{
  "name": "visitor-backend",
  "version": "1.0.0",
  "description": "Node Backed Service For Visitor Notification",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env-shell NODE_ENV=test \"npm run test:resetdb && nyc ./node_modules/.bin/mocha\"",
    "test:ci": "cross-env-shell NODE_ENV=test \"npm run test:resetdb && ./node_modules/.bin/mocha\"",
    "test:lite": "cross-env-shell NODE_ENV=test \"./node_modules/.bin/mocha\"",
    "test:resetdb": "cross-env-shell NODE_ENV=test \"npm run reset:db && npm run setup:db\"",
    "lint": "./node_modules/.bin/eslint --ext .ts src && tsc",
    "lint:stats": "./node_modules/.bin/eslint --format ./node_modules/eslint-stats/byError.js --ext .ts src",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .ts src",
    "build": "./node_modules/.bin/tsc && copyfiles -u 1 \"src/**/*.pug\" dist/",
    "dev:start": "npm-run-all build start",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "node .",
    "worker": "node dist/config/worker.js",
    "worker-dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/config/worker.ts",
    "new:module": "./node_modules/.bin/hygen entity new",
    "new:sub": "./node_modules/.bin/hygen entity sub",
    "new:model": "./node_modules/.bin/hygen entity model",
    "reset:db": "./node_modules/.bin/sequelize-cli db:seed:undo:all && ./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "new:migration": "./node_modules/.bin/sequelize-cli migration:generate --name",
    "new:seeder": "./node_modules/.bin/sequelize-cli seed:generate --name",
    "migrate": "./node_modules/.bin/sequelize-cli db:migrate",
    "rollback": "./node_modules/.bin/sequelize-cli db:migrate:undo",
    "seed:all": "./node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "./node_modules/.bin/sequelize-cli db:seed:undo",
    "setup:db": "./node_modules/.bin/sequelize-cli db:migrate && ./node_modules/.bin/sequelize-cli db:seed:all"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tinkeshwar/node-admin-framework.git"
  },
  "author": "Tinkeshwar Singh",
  "license": "ISC",
  "dependencies": {
    "@bugsnag/js": "^7.11.0",
    "@hapi/hapi": "^20.1.5",
    "@hapi/inert": "^6.0.4",
    "@hapi/jwt": "^2.1.0",
    "@hapi/vision": "^6.1.0",
    "aws-sdk": "^2.989.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "bull": "^3.29.2",
    "busboy": "^0.3.1",
    "camelcase": "^6.2.0",
    "camelcase-keys": "^6.2.2",
    "cross-env": "^7.0.3",
    "crypto-random-string": "^3.3.1",
    "dateformat": "^4.5.1",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "hapi-auth-jwt2": "^10.2.0",
    "image-size": "^1.0.0",
    "ioredis": "^4.27.9",
    "joi": "^17.4.2",
    "lodash": "^4.17.20",
    "mailgun-js": "^0.22.0",
    "mysql2": "^2.2.5",
    "node-cleanup": "^2.1.2",
    "path": "^0.12.7",
    "pdfkit": "^0.12.3",
    "pidusage": "^2.0.21",
    "pug": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "snakecase-keys": "^3.2.0",
    "to-snake-case": "^1.0.0",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/bluebird": "^3.5.36",
    "@types/bull": "^3.15.3",
    "@types/busboy": "^0.2.4",
    "@types/chai": "^4.2.21",
    "@types/dateformat": "^3.0.1",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/fs-extra": "^9.0.12",
    "@types/hapi__hapi": "^20.0.9",
    "@types/hapi__inert": "^5.2.3",
    "@types/hapi__joi": "^17.1.7",
    "@types/hapi__jwt": "^2.0.1",
    "@types/hapi__vision": "^5.5.3",
    "@types/ioredis": "^4.27.1",
