    "eslint-plugin-promise": "^5.1.0",
    "fs-extra": "^9.0.1",
    "hapi-swagger": "^14.2.4",
    "hygen": "^6.1.0",
    "mocha": "^9.1.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
